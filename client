CreateThread(function()
while true do
local seats = {
            [0] = 'delta@drive_by@std_ps_1h',
            [1] = 'delta@drive_by@std_rds_1h',
            [2] = 'delta@drive_by@std_rps_1h',
        }
        for _, anim in pairs(seats) do
            RequestAnimSet(anim)
        end
Wait(10000)
end
end)
